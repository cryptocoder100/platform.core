{
  "Cosmos": {
    "AdminRepositoryOptions": {
      "database": "EXOSEntityManagementDB",
      "collection": "ReferenceDataCollection",
      "endpoint": "${Cosmos:Endpoint}",
      "AuthKey": "${Cosmos:ReadWriteKey}",
      "connectionMode": "Direct",
      "partitionKey": "partitionKey",
      "forceConcurrencyCheck": false,
      "captureQueryMetrics": true,
      "applyDocumentPolicy": true
    }
  },
  "Redis": {
    "ConnectionPoolEnabled": "${Redis:ConnectionPoolEnabled}",
    "MaxRetries": "${Redis:MaxRetries}",
    "PoolSize": "${Redis:PoolSize}",
    "ReadWriteConnectionString": "${Redis:ReadWriteConnectionString}",
    "keyPrefix": "",
    "expiryDay": 1,
    "useInMemoryCache": true,
    "pagenationExpireSeconds": "600"
  }
}
